<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pdmtrabalho">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ProdutoScene"
            android:label="@string/title_activity_produto_scene"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".CategoriaScene"
            android:label="@string/title_activity_categoria_scene"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ItemProduto"
            android:label="@string/title_activity_item_produto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CategoriaItem" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>